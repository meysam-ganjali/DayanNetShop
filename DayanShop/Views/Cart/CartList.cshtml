@model DayanShop.Domains.Entities.Cart
@{
    ViewData["Title"] = "سبد خرید";
    long totalAmount = 0;
    long? totalDiscount = 0;
}
<section class="inner-page" id="cart-page">
    <div class="container-fluid" id="page-hero">
        <div class="row">
            <div class="col-12">
                <div class="container">
                    <div class="row">
                        <div class="col-12 px-0">
                            <h1>@ViewData["Title"]</h1>
                            <p>مدیریت و خرید همزمان چند محصول</p>
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="~/">صفحه نخست</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">سبد خرید</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="content">
                    <div class="row">
                        <div class="col-12 col-lg-9">
                            <div id="cart-products">
                                <div class="container">
                                    <div class="row">
                                        <div class="col-12 py-3">
                                            <div class="pb-3" id="return-to-shop">می‌خواهید محصولات دیگری اضافه کنید؟ <a asp-controller="Shop" asp-action="Index">بازگشت به فروشگاه</a></div>
                                            <div class="d-none d-md-block">
                                                <div class="row my-2" id="heading">
                                                    <div class="col-4">
                                                        <div>کالا</div>
                                                    </div>
                                                    <div class="col-2">
                                                        <div>قیمت واحد</div>
                                                    </div>
                                                    <div class="col-2 pl-4">
                                                        <div>تعداد</div>
                                                    </div>
                                                    <div class="col-2">
                                                        <div>تخفیف</div>
                                                    </div>
                                                    <div class="col-2 pr-0">
                                                        <div class="pr-3">قیمت نهایی</div>
                                                    </div>
                                                </div>
                                            </div>
                                            @if (Model == null)
                                            {
                                                <p class="text-center text-danger">محصولی در سبد شما یافت نشد</p>
                                            }
                                            else
                                            {
                                                <!-- Order Product Record -->
                                                foreach (var item in Model.CartItems.ToList())
                                                {
                                                    <div class="row product">
                                                        <div class="col-12 col-md-4">
                                                            <div class="row">
                                                                <div class="col-2 col-md-4 pl-0">
                                                                    <img src="~/@item.Product.ProductImages.Select(p=>p.ImagePath).FirstOrDefault()" alt="">
                                                                </div>
                                                                <div class="col-10 col-md-8">
                                                                    <a asp-controller="Shop" asp-action="ProductDetaile" asp-route-id="@item.ProductId" target="_blank"><div class="title pt-2">@item.Product.Name</div></a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-6 col-md-2">
                                                            <div class="d-md-none font-weight-bold">قیمت</div>
                                                            <div class="pt-1"><small class="product-encode4365gbf265g43d">@item.Product.Price.ToString("#,##")</small> <small class="text-decoration-line-through text-danger">@item.Product.OldPrice </small><small>تومان</small></div>
                                                        </div>
                                                        <div class="col-6 col-md-2 pl-4 pr-0 pr-md-3">
                                                            <div class="d-md-none font-weight-bold">تعداد</div>
                                                            <div class="input-group mb-3 order-number">
                                                                <div class="input-group mb-3">
                                                                    <a asp-controller="Cart" asp-action="Add" asp-route-id="@item.Id" class="input-group-text btn btn-sm btn-outline-primary">+</a>
                                                                    <input type="text" value="@item.Count" class="form-control input-sm" aria-label="Amount (to the nearest dollar)">
                                                                    <a asp-controller="Cart" asp-action="Sub" asp-route-id="@item.Id" class="input-group-text btn btn-sm btn-outline-danger">-</a>
                                                                </div>


                                                            </div>
                                                        </div>
                                                        <div class="col-6 col-md-2">
                                                            <div class="d-md-none font-weight-bold">تخفیف</div>
                                                            <div class="pt-1"><span class="product-discount">@((item.Product.OldPrice - item.Product.Price) * item.Count)</span> <span>تومان</span></div>
                                                        </div>
                                                        <div class="col-6 col-md-2 pr-0">
                                                            <div class="d-md-none font-weight-bold">قیمت نهایی</div>
                                                            <div class="pt-1 pr-2 bg-light"><span class="product-total">@((item.Count * item.Product.Price).ToString("#,##"))</span> <span>تومان</span></div>
                                                            <a asp-controller="Cart" asp-action="RemoveItem" asp-route-id="@item.Id" class="btn btn-link text-danger text-decoration-none"><small> حذف آیتم ×</small></a>
                                                            @{
                                                                totalAmount += item.Product.Price * item.Count;
                                                                if (item.Product.OldPrice > 0)
                                                                {
                                                                    totalDiscount += ((item.Product.OldPrice - item.Product.Price) * item.Count);
                                                                }

                                                            }
                                                        </div>
                                                    </div>
                                                    <hr>
                                                }

                                                <!-- Order Product Record -->
                                                <hr>
                                                <!-- Order Product Record -->
                                                <div class="row product">
                                                    <div class="col-12">
                                                        <a onclick="RemoveCart('@Model.Id')" class="product-remove btn-remove-from-basket" data-id="all"><div class="float-end small pl-2 font-weight-bold">خالی کردن سبد</div></a>
                                                    </div>
                                                </div>
                                            }


                                        </div>
                                    </div>
                                </div>
                            </div>
